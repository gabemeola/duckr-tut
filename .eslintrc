{
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"plugins": [
		"react"
	],
	"env": {
		"es6": true,
		"browser": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"experimentalObjectRestSpread": true
		}
	},
	"globals": {
		"document": true,
		"window": true
	}
}